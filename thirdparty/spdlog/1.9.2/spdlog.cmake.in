# Cppkg Base Dependency Downloader
cmake_minimum_required(VERSION 3.6)
project(spdlog-1.9.2-install)

set(CPPM_VERSION ${CPPM_VERSION})
include(${CMAKE_CURRENT_SOURCE_DIR}/cmake/cppm_loader.cmake)
download_package(spdlog 1.9.2  URL https://github.com/gabime/spdlog/archive/refs/tags/v1.9.2.zip  TYPE lib CMAKE_ARGS ${CMAKE_ARGS} )

